{
  "version": "2.0.0",
  "showOutput": "silent",
  "tasks": [
    {
      "taskName": "build",
      "problemMatcher": "$go",
      "isBuildCommand": true,
      "command": "go",
      "args": [
        "run",
        "./build.go"
      ],
      "isShellCommand": false
    },
    {
      "taskName": "gen",
      "command": "go",
      "args": [
        "run", "./build.go", "-with-gogen"
      ],
      "problemMatcher": "$go",
      "isShellCommand": false
    }
  ]
}
